<idea-plugin>
    <id>io.github.elena-lyulina.codetracker.codetracker</id>
    <name>codetracker</name>
    <vendor email="elena.lyulina@gmail.com" url="https://github.com/elena-lyulina"> Elena Lyulina </vendor>

    <description><![CDATA[
    This is a plugin for tracking all code changes and storing them in .csv file.
    ]]></description>


    <depends>com.intellij.modules.platform</depends>

    <extensions defaultExtensionNs="com.intellij">
        <postStartupActivity implementation="InitActivity"></postStartupActivity>
        <toolWindow id="codetracker" anchor="right" factoryClass="ui.PluginToolWindowFactory"/>
    </extensions>

</idea-plugin>